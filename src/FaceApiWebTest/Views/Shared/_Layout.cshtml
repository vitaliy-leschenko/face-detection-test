<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Face Api</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
</head>
<body>
    <header>
        <ul>
            <li><a asp-area="" asp-controller="Home" asp-action="Azure">Azure</a></li>
            <li><a asp-area="" asp-controller="Home" asp-action="Google">Google</a></li>
            <li><a asp-area="" asp-controller="Home" asp-action="Amazon">Amazon</a></li>
        </ul>
    </header>

    <div class="container body-content">
        @RenderBody()
    </div>

    <script src="~/lib/requirejs/require.js"></script>
    <script src="~/js/app.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
